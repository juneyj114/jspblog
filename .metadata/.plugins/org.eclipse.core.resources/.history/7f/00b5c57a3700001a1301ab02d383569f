<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <title>Jquery</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script>
      $(document).ready(() => {
        $.ajax("/blog/AjaxTest3", {
          type: "POST",
          dataType: "text",
          contentType: "text/plain; charset=UTF-8",
          data: "Hello"
        }).done(data => {
        	$("#demo").text(data);
        });
      });
    </script>
  </head>
  <body>
    <h1 id="demo"></h1>
  </body>
</html>
